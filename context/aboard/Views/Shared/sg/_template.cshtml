    "@@type": "Corporation",
    "name": "@ViewBag.site_name",
    "url": "@Request.Url.AbsoluteUri",
    "logo": "@Request.Url.GetLeftPart(UriPartial.Authority)@Url.Content("~/OVSWS/Content/img/"+@ViewBag.country_url+"/"+@ViewBag.city_url+"/header/logo-lg.svg")",
    "image": "@Request.Url.GetLeftPart(UriPartial.Authority)@Url.Content("~/OVSWS/Content/img/"+@ViewBag.country_url+"/"+@ViewBag.city_url+"/kv/bg-lg.jpg")",
    "description": "@ViewBag.PageDescription",
    "address": {
        "@@type": "PostalAddress",
        "streetAddress": "8 Marina Boulevard #13-01/03, Marina Bay Financial Centre, Singapore 018981"
    },
    "contactPoint": {
        "@@type": "ContactPoint",
        "telephone": "+65 6593 9280"
    }
    }
    </script>

    

    <style>
        .component-header .ch-brand{
            padding-right:0;
        }
        .component-header .ch-brand .ch-img {
            width: 230px;
            height: 24px;
        }

        @@media (min-width: 992px) {
            .component-header .ch-brand .ch-img {
                width: 300px;
                height: auto;
            }
        }
        

    </style>
</head>
<body class='@ViewBag.html_lang'>
    @RenderPage("header.cshtml")
    @RenderPage(ViewBag.bodyPath, new { ViewBag = ViewBag })
    @RenderPage("fraud-prevention-window.cshtml")
    @RenderPage("exit-window.cshtml")
    @RenderPage("footer.cshtml")

    <a href="javascript:;" class="component-gototop" role="page-go-top"></a>
    <script>
        window.faq_title = "@ViewBag.faq_title";
        window.download_title = "@ViewBag.download_title";
    </script>
    <script>
        function logElementEvent(eventName, element) {
            //console.log(Date.now(), eventName, element.getAttribute("data-src"));
        }

        var callback_enter = function (element) {
            logElementEvent("🔑 ENTERED", element);
        };
        var callback_exit = function (element) {
            logElementEvent("🚪 EXITED", element);
        };
        var callback_loading = function (element) {
            logElementEvent("⌚ LOADING", element);
        };
        var callback_loaded = function (element) {
            logElementEvent("👍 LOADED", element);
        };
        var callback_error = function (element) {
            logElementEvent("💀 ERROR", element);
            element.src =
                "https://via.placeholder.com/440x560/?text=Error+Placeholder";
        };
        var callback_finish = function () {
            logElementEvent("✔️ FINISHED", document.documentElement);
        };
        var callback_cancel = function (element) {
            logElementEvent("🔥 CANCEL", element);
        };

        window.lazyLoadOptions = {
            threshold: 0,
            // Assign the callbacks defined above
            callback_enter: callback_enter,
            callback_exit: callback_exit,
            callback_cancel: callback_cancel,
            callback_loading: callback_loading,
            callback_loaded: callback_loaded,
            callback_error: callback_error,
            callback_finish: callback_finish
        };
        window.addEventListener(
            "LazyLoad::Initialized",
            function (e) {
                console.log(e.detail.instance);
            },
            false
        );
    </script>
    <script type="text/javascript" src="@Url.Content("~/OVSWS/Content/js/lib/jquery-3.6.0.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/OVSWS/Content/js/lib/lazyload.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/OVSWS/Content/js/lib/picturePolyfill.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/OVSWS/Content/js/component/header/main.js")"></script>

    <script type="text/javascript" src="@Url.Content("~/OVSWS/Content/js/app.js")"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
